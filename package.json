{
  "name": "nuxt-contentful-starter",
  "version": "1.0.0",
  "description": "A static blog based on Contentful",
  "author": "David Couronn√© <couronne.david@gmail.com>",
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "babel-runtime": "^6.26.0",
    "contentful": "^7.0.6",
    "contentful-management": "^5.4.0",
    "nuxt": "^2.3.4",
    "vue-markdown": "^2.2.4"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "nuxt generate && now dist",
    "import-data": "node ./bin/download-content-model.js && contentful-import --content-file ./data/blog/contentful-export.json"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "contentful-import": "^7.4.3",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "listr": "^0.14.3",
    "node-fetch": "^2.3.0",
    "now": "^13.0.2",
    "rimraf": "^2.6.1",
    "tar": "^4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
